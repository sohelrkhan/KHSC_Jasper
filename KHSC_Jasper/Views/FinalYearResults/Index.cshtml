@model IEnumerable<KHSC_Jasper.Models.FinalYearResult>

@{
    ViewData["Title"] = "Index";
}
<link href="~/css/form.css" rel="stylesheet" />
<link href="~/css/table.css" rel="stylesheet" />
<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ct2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sub)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Obj)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Practical)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Attendance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Exam)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Student)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Subject)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Teacher)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Ct2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Sub)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Obj)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Practical)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Attendance)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Exam.ExamName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.StudentName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Subject.SubjectName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Teacher.Email)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Fy">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Fy">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Fy">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
